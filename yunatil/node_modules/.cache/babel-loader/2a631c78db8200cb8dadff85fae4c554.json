{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  data: function data() {\n    return {\n      title: \"Today I Learned\",\n      input: \"abc\",\n      options: [{\n        v: \"c\",\n        t: 'd1'\n      }, {\n        v: \"s\",\n        t: \"d2\"\n      }, {\n        v: \"e\",\n        t: \"d3\"\n      }],\n      region: \"s\",\n      tableShow: false\n    };\n  },\n  watch: {\n    //특정 데이터를 계속 모니터링 하다가 변경이 일어나면 작업을 진행해줌\n    //변수명과 동일한 이름으로 메서드를 만들어주면 됨\n    input: function input() {\n      console.log(this.input);\n    }\n  },\n  methods: {\n    getData: function getData() {\n      alert(this.input);\n    },\n    setData: function setData() {\n      this.input = \"12345\";\n    },\n    changeRegin: function changeRegin() {\n      alert(this.region);\n    }\n  },\n  beforeCreate: function beforeCreate() {\n    console.log(\"beforeCreate\");\n  },\n  created: function created() {\n    //db에서 가져온 데이터를 화면에 뿌려줘야하는 경우 \n    console.log(\"created\"); //미리 db에서 데이터를 가져와서 data()에 바인딩 해주면 됨\n  },\n  beforeMount: function beforeMount() {\n    console.log(\"beforeMount\");\n  },\n  mounted: function mounted() {\n    //mounted일 때 화면에 띄워줌\n    //mounted된 이후에 데이터를 가지고 와서 데이터를 설정하면 beforeUpdate와 updated가 불필요하게 실행됨.. \n    console.log(\"mounted\");\n  },\n  beforeUpdate: function beforeUpdate() {\n    console.log(\"beforeUpdate\"); //데이터 변경 전\n  },\n  updated: function updated() {\n    console.log(\"updated\"); //데이터 변경 후\n  },\n  beforeDestory: function beforeDestory() {\n    console.log(\"beforeDestory\");\n  },\n  destroyed: function destroyed() {\n    console.log(\"destroyed\");\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;AAsBA;AACAA,MADA,kBACA;AACA;AACAC,8BADA;AAEAC,kBAFA;AAGAC,gBACA;AAAAC;AAAAC;AAAA,OADA,EAEA;AAAAD;AAAAC;AAAA,OAFA,EAGA;AAAAD;AAAAC;AAAA,OAHA,CAHA;AAQAC,iBARA;AASAC;AATA;AAWA,GAbA;AAcAC;AACA;AACA;AACAN,SAHA,mBAGA;AACAO;AACA;AALA,GAdA;AAqBAC;AACAC,WADA,qBACA;AACAC;AACA,KAHA;AAIAC,WAJA,qBAIA;AACA;AACA,KANA;AAOAC,eAPA,yBAOA;AACAF;AACA;AATA,GArBA;AAgCAG,cAhCA,0BAgCA;AACAN;AACA,GAlCA;AAmCAO,SAnCA,qBAmCA;AACA;AACAP,2BAFA,CAEA;AACA,GAtCA;AAuCAQ,aAvCA,yBAuCA;AACAR;AACA,GAzCA;AA0CAS,SA1CA,qBA0CA;AACA;AACA;AACAT;AACA,GA9CA;AA+CAU,cA/CA,0BA+CA;AACAV,gCADA,CACA;AACA,GAjDA;AAkDAW,SAlDA,qBAkDA;AACAX,2BADA,CACA;AACA,GApDA;AAqDAY,eArDA,2BAqDA;AACAZ;AACA,GAvDA;AAwDAa,WAxDA,uBAwDA;AACAb;AACA;AA1DA","names":["data","title","input","options","v","t","region","tableShow","watch","console","methods","getData","alert","setData","changeRegin","beforeCreate","created","beforeMount","mounted","beforeUpdate","updated","beforeDestory","destroyed"],"sourceRoot":"src/views","sources":["Home.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <h1>{{title}}</h1>\r\n        <input type=\"text\" v-model=\"input\"/>\r\n        <button type=\"button\" @click=\"getData\">Get</button>\r\n        <button type=\"button\" @click=\"setData\">Set</button>\r\n\r\n        <select class=\"form-control\" v-model=\"region\" @change=\"changeRegin\">\r\n            <option :key=\"i\" :value=\"d.v\"  v-for=\"(d, i) in options\">{{d.t}}</option>\r\n        </select>\r\n        \r\n        <!-- v-if는 조건이 맞지 않으면 아예 랜더링 하지 않는다.\r\n        v-show는 랜더링 하는데 단지 css display값으로 보이는지 아닌지 설정해주는 차이일 뿐이다. -->\r\n        <table class=\"table table-bordered\" v-show=\"tableShow\">\r\n            <tr :key=\"i\" v-for=\"(d, i) in options\">\r\n                <td>{{d.v}}</td>\r\n                <td>{{d.t}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n    data(){\r\n        return {\r\n            title: \"Today I Learned\",\r\n            input: \"abc\",\r\n            options: [\r\n                {v: \"c\",t:'d1'},\r\n                {v:\"s\",t:\"d2\"},\r\n                {v:\"e\", t:\"d3\"}\r\n            ],\r\n            region: \"s\",\r\n            tableShow: false,\r\n        }\r\n    },\r\n    watch: {\r\n        //특정 데이터를 계속 모니터링 하다가 변경이 일어나면 작업을 진행해줌\r\n        //변수명과 동일한 이름으로 메서드를 만들어주면 됨\r\n        input(){\r\n            console.log(this.input)\r\n        }\r\n    },\r\n    methods: {\r\n        getData(){\r\n            alert(this.input)\r\n        },\r\n        setData(){\r\n            this.input = \"12345\"\r\n        },\r\n        changeRegin(){\r\n            alert(this.region)\r\n        }\r\n    },\r\n    beforeCreate(){\r\n        console.log(\"beforeCreate\")\r\n    },\r\n    created(){\r\n        //db에서 가져온 데이터를 화면에 뿌려줘야하는 경우 \r\n        console.log(\"created\")  //미리 db에서 데이터를 가져와서 data()에 바인딩 해주면 됨\r\n    },\r\n    beforeMount(){\r\n        console.log(\"beforeMount\")\r\n    },\r\n    mounted(){\r\n        //mounted일 때 화면에 띄워줌\r\n        //mounted된 이후에 데이터를 가지고 와서 데이터를 설정하면 beforeUpdate와 updated가 불필요하게 실행됨.. \r\n        console.log(\"mounted\")\r\n    },\r\n    beforeUpdate(){\r\n        console.log(\"beforeUpdate\") //데이터 변경 전\r\n    },\r\n    updated(){\r\n        console.log(\"updated\")  //데이터 변경 후\r\n    },\r\n    beforeDestory(){\r\n        console.log(\"beforeDestory\")\r\n    },\r\n    destroyed(){\r\n        console.log(\"destroyed\")\r\n    }\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}